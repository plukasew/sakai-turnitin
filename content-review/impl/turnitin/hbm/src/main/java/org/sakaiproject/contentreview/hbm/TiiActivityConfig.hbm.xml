<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.sakaiproject.turnitin.api.TiiActivityConfig"
		table="TII_ACTIVITY_CONFIG">

		<id name="id" type="long">
			<generator class="native">
				<param name="sequence">TII_ACTIVITY_CONFIG_SEQ</param>
			</generator>
		</id>

		<property name="toolId" type="string" length="255" not-null="true"/>
		<property name="activityId" type="string" length="255" not-null="true" index="turnitin_activityId"/>
		<property name="stealthedLtiId" type="string" length="255" not-null="true"/>
		<property name="turnitinAssignmentId" type="string" length="255" not-null="false"/>
		<property name="lastestIndividualExtensionDate" type="java.util.Date" not-null="false"/>
	</class>
</hibernate-mapping>
